<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Iniciar_Sesion_Medicos</servlet-name>
        <servlet-class>controlador.Medicos.Iniciar_Sesion_Medicos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Buscar_Medico_ID</servlet-name>
        <servlet-class>controlador.Medicos.Buscar_Medico_ID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editar_Medico</servlet-name>
        <servlet-class>controlador.Medicos.Editar_Medico</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Iniciar_Sesion_Medicos</servlet-name>
        <url-pattern>/Iniciar_Sesion_Medicos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Buscar_Medico_ID</servlet-name>
        <url-pattern>/Buscar_Medico_ID</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editar_Medico</servlet-name>
        <url-pattern>/Editar_Medico</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
